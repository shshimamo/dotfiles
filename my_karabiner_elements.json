          "to_if_alone": [
            { "set_variable": { "name": "vk2", "value": 1 } }
          ],
          "to_delayed_action": {
            "to_if_invoked": [
              { "set_variable": { "name": "vk2", "value": 0 } }
            ],
            "to_if_canceled": [
              { "set_variable": { "name": "vk2", "value": 0 } }
            ]
          }
        }
      ]
    },

