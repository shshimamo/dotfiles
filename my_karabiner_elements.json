{
  "title": "自分オリジナル設定",
  "rules": [
    {
      "description": "コマンドを単体で英数/かな（左は英数、右はかな） (lazy: falseに変更)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_command",
            "modifiers": {
              "optional": [ "any" ]
            }
          },
          "to": [
            {
              "key_code": "left_command",
              "lazy": false
            }
          ],
          "to_if_alone": [
            { "key_code": "japanese_eisuu" }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "right_command",
            "modifiers": {
              "optional": [ "any" ]
            }
          },
          "to": [
            {
              "key_code": "right_command",
              "lazy": false
            }
          ],
          "to_if_alone": [
            { "key_code": "japanese_kana" }
          ]
        }
      ]
    },

    {
      "description": "Shift_L + hjkl => 矢印キー",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": [
                "left_shift"
              ]
            }
          },
          "to": [
            { "key_code": "left_arrow" }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": [
                "left_shift"
              ]
            }
          },
          "to": [
            { "key_code": "down_arrow" }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": [
                "left_shift"
              ]
            }
          },
          "to": [
            { "key_code": "up_arrow" }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "l",
            "modifiers": {
              "mandatory": [
                "left_shift"
              ]
            }
          },
          "to": [
            { "key_code": "right_arrow" }
          ]
        }
      ]
    },

    {
      "description": "CapsLockを単体はESC, 組み合わせはControl",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "caps_lock",
            "modifiers": { "optional": [ "any" ] }
          },
          "to": [
            { "key_code": "left_control" }
          ],
          "to_if_alone": [
            { "key_code": "escape" }
          ]
        }
      ]
    },

    {
      "description": "左Control単体をESC",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_control",
            "modifiers": { "optional": [ "any" ] }
          },
          "to": [
            { "key_code": "left_control" }
          ],
          "to_if_alone": [
            { "key_code": "escape" }
          ]
        }
      ]
    },

    {
      "description": "右Shift単体をvk1, 2回でLastApp",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "right_shift",
            "modifiers": { "optional": [ "any" ] }
          },
          "to": [
            {
              "key_code": "tab",
              "modifiers": ["left_command"]
            },
            { "set_variable": { "name": "vk1", "value": 0 } }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "vk1",
              "value": 1
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "right_shift",
            "modifiers": { "optional": [ "any" ] }
          },
          "to": [
            { "key_code": "right_shift" }
          ],
          "to_if_alone": [
            { "set_variable": { "name": "vk1", "value": 1 } }
          ],
          "to_delayed_action": {
            "to_if_invoked": [
              { "set_variable": { "name": "vk1", "value": 0 } }
            ],
            "to_if_canceled": [
              { "set_variable": { "name": "vk1", "value": 0 } }
            ]
          }
        }
      ]
    },

    {
      "description": "左Shift単体をvk2",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_shift",
            "modifiers": { "optional": [ "any" ] }
