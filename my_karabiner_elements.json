{
  "title": "自分オリジナル設定",
  "rules": [
    {
      "description": "左Cmd単体 => [英数]、右Cmd単体 => [かな]",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_command",
            "modifiers": {
              "optional": [ "any" ]
            }
          },
          "to": [
            {
              "key_code": "left_command",
              "lazy": false
            }
          ],
          "to_if_alone": [
            { "key_code": "japanese_eisuu" }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "right_command",
            "modifiers": {
              "optional": [ "any" ]
            }
          },
          "to": [
            {
              "key_code": "right_command",
              "lazy": false
            }
          ],
          "to_if_alone": [
            { "key_code": "japanese_kana" }
          ]
        }
      ]
    },

    {
      "description": "左Shift単体 => [英数]",
