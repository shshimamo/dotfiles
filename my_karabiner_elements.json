    {
      "description": "[Chrome] Bind 'Cmd + i' as 'Cmd + Option + i'",
      "manipulators": [
        {
          "conditions": [
            {
              "bundle_identifiers": ["^com\\.google\\.Chrome"],
              "type": "frontmost_application_if"
            }
          ],
          "from": {
            "key_code": "i",
            "modifiers": {
              "mandatory": ["command"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "i",
              "modifiers": ["command", "left_option"]
            }
          ],
          "type": "basic"
        }
      ]
    },

    {
      "description": "[Chrome] Bind 'Cmd + d' as 'Shift + Cmd + d'",
