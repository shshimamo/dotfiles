    {
      "description": "[Kinesis]右Optionを 単体=>ESC, +R/T/F/C=> Rubymine, iTerm, Chrome, VSCode",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "right_option",
            "modifiers": { "optional": [ "any" ] }
          },
          "to": [
            { "set_variable": { "name": "vk1", "value": 1 } }
          ],
          "to_after_key_up": [
            { "set_variable": { "name": "vk1", "value": 0 } }
          ],
          "to_if_alone": [
            { "key_code": "escape" }
          ]
        },

        {
          "type": "basic",
          "conditions": [
            { "type": "variable_if", "name": "vk1", "value": 1 },
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.google\\.Chrome",
                "^com\\.googlecode\\.iterm2",
                "^com\\.happenapps\\.Quiver",
                "^com\\.microsoft\\.VSCode",
                "^com\\.tinyspeck\\.slackmacgap",
                "^com\\.kapeli\\.dashdoc",
